#!/usr/bin/env ruby
require 'rubygems'
require 'RMagick'

require 'ruby-debug'

$:.unshift(File.expand_path("../../lib", __FILE__))
require 'qrio'

@source = Magick::Image.read(ARGV.shift).first

finder_patterns = Qrio::FinderPattern.extract(@source)
finder_patterns.each do |fp|
  puts "found pattern at #{ fp.center.first }, #{ fp.center.last } (#{ fp.width }x#{ fp.height })"
end
